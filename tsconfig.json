{
  "compilerOptions": {
    "target": "es6",
    "module": "esnext",
    "lib": ["es2017", "dom", "esnext"],
    "jsx": "react-jsx",
    "strict": true,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "types": ["@figma/plugin-typings"]
  },
  "include": ["src", "example", "test"]
}
